<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainLayout" prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.power_systems_modelica.psm.gui.view.MainLayoutController">
   <children>
      <Button fx:id="mainAction" layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleMainAction" styleClass="btn-primary" AnchorPane.rightAnchor="14.0" />
      <MenuButton fx:id="menuActions" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" styleClass="btn-default" AnchorPane.leftAnchor="14.0" />
      <TitledPane fx:id="summaryPane" collapsible="false" layoutX="14.0" layoutY="53.0" prefHeight="0.0" text="Summary" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0">
         <content>
            <AnchorPane fx:id="summaryContent" prefHeight="200.0" prefWidth="200.0" />
         </content>
      </TitledPane>
   </children>
   <stylesheets>
      <URL value="@/css/MainApp.css" />
      <URL value="@/css/bootstrap3.css" />
   </stylesheets>
</AnchorPane>
