                                  <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
                                  <TableColumn fx:id="rmseColumn" prefWidth="75.0" text="RMSE" />
                                  <TableColumn fx:id="rdColumn" prefWidth="75.0" text="RD" />
                                  <TableColumn fx:id="adColumn" prefWidth="75.0" text="AD" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children></AnchorPane>
                    </content>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding></TitledPane>
               </children>
            </AnchorPane>
         </content>
      </ScrollPane>
   </children>
   <stylesheets>
      <URL value="@/css/bootstrap3.css" />
      <URL value="@/css/MainApp.css" />
   </stylesheets>
