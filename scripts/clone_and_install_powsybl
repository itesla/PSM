rm -rf temp_powsybl
mkdir -p temp_powsybl
cd temp_powsybl
git clone https://github.com/powsybl/powsybl-core.git
cd powsybl-core
git checkout tags/v1.0.0
mvn clean install -DskipTests
cd ..
cd ..
rm -rf temp_powsybl
